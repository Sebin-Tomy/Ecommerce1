<script>
    function handleApprovalRequest(orderId) {
        swal({
            title: "Are you sure?",
            text: "Do you want to request approval for return?",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
        .then((willRequest) => {
            if (willRequest) {
                fetch('/admin/submitReturn1', {
                    method: 'PATCH',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({ orderId: orderId }),
                })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        swal("Success!", "Approval request sent.", "success");
                        window.location.reload()
                        
                    } else {
                        swal("Error!", "Failed to send approval request.", "error");
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    swal("Error!", "Failed to send approval request.", "error");
                });
            } else {
                swal("Approval request canceled.");
            }
        });
    }
</script>

